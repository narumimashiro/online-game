{
  "extends": [
    "next/core-web-vitals",
    "plugin:storybook/recommended"
  ],
  "plugins": [
    "import",
    "unused-imports"
  ],
  "rules": {
    "no-unused-expressions": "error",
    "no-empty": [
      "error",
      {
        "allowEmptyCatch": true
      }
    ],
    "no-console": "error",
    "@next/next/no-img-element": "off",
    "unused-imports/no-unused-imports": "error"
  }
}
